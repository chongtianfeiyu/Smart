<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
     http://www.springframework.org/schema/beans   
     http://www.springframework.org/schema/beans/spring-beans.xsd">

     <!-- 基础应用平台-退款服务 -->
	<bean id="refundResource" class="com.ips.rest.core.spring.RestfulFactoryBean">
        <property name="serviceInterface" value="com.ips.core.psfp.refund.biz.refund.resource.RefundResource" /> 
        <property name="serviceUrl" value="${refund.interface.url}" />
	</bean>
	
	<!-- 基础应用平台-出款服务 -->
	<bean id="issuedCallBackResource" class="com.ips.rest.core.spring.RestfulFactoryBean">
        <property name="serviceInterface" value="com.ips.core.psfp.issued.biz.issued.resource.IssuedCallBackResource" /> 
        <property name="serviceUrl" value="${issued.interface.url}" />
	</bean>
    
</beans> 